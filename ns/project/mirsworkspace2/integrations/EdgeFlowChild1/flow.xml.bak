<?xml version="1.0" encoding="UTF-8"?>

<FLOW VERSION="1.0" CLEANUP="false">
  <COMMENT></COMMENT>

  <!-- nodes -->

<INVOKE NAME="Date" SERVICE="pub.date:getCurrentDateString">
  <COMMENT></COMMENT>

  <!-- nodes -->

<MAP MODE="INPUT">
  <COMMENT></COMMENT>

  <!-- nodes -->

<MAPSET NAME="Setter" OVERWRITE="true" VARIABLES="true" GLOBALVARIABLES="false" FIELD="/pattern;1;0">
  <DATA ENCODING="XMLValues" I18N="true">

<Values version="2.0">
  <value name="xml">YYYY-MM-DD HH:mm:ss</value>
</Values>
</DATA>
</MAPSET>
</MAP>

<MAP MODE="OUTPUT">
  <COMMENT></COMMENT>
</MAP>
</INVOKE>

<INVOKE NAME="Flow" SERVICE="pub.flow:debugLog">
  <COMMENT></COMMENT>

  <!-- nodes -->

<MAP MODE="INPUT">
  <COMMENT></COMMENT>

  <!-- nodes -->

<MAPSET NAME="Setter" OVERWRITE="true" VARIABLES="false" GLOBALVARIABLES="false" FIELD="/message;1;0">
  <DATA ENCODING="XMLValues" I18N="true">

<Values version="2.0">
  <value name="xml">Edge Runtime Flow Child Log : Date is %value%</value>
</Values>
</DATA>
</MAPSET>
</MAP>

<MAP MODE="OUTPUT">
  <COMMENT></COMMENT>
</MAP>
</INVOKE>
</FLOW>
